pipeline {
  agent any
  stages {
    stage('Prepration') {
      steps {
        echo 'It is the preparation step.'
      }
    }
    stage('Build') {
      steps {
        echo 'Project will be hare'
      }
    }
    stage('test') {
      steps {
        echo 'testing'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deploy done'
      }
    }
    stage('Deploy Done') {
      steps {
        echo 'Done'
      }
    }
  }
}